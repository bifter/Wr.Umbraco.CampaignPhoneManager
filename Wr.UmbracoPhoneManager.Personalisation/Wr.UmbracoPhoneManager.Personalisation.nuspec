<?xml version="1.0"?>
<package >
  <metadata>
    <id>UmbracoPhoneManagerPersonalisationGroupsPlugin</id>
    <version>$version$</version>
    <title>Umbraco Phone Manager Personalisation Groups plugin</title>
    <authors>Will Roscoe</authors>
    <owners>Will Roscoe</owners>
    <licenseUrl>https://github.com/willroscoe/UmbracoPhoneManager/blob/master/LICENSE.md</licenseUrl>
    <projectUrl>https://github.com/willroscoe/UmbracoPhoneManager</projectUrl>
    <iconUrl>https://github.com/willroscoe/UmbracoPhoneManager/raw/master/assets/umbracophonemanager_icon64.png</iconUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Phone Manager Personalisation Groups plugin is an Umbraco package to add a Phone Manager criteria item to the Personalisation Groups package. Using this it is possible to personalise your site based on the selected Phone Manager campaign.</description>
    <tags>Umbraco Marketing Campaign Telephone Phone Manager Personalisation Groups plugin</tags>
    <dependencies>
      <dependency id="UmbracoCms.Core" version="8.1.0" />
	  <dependency id="Zone.UmbracoPersonalisationGroups" version="2.1.1" />
    </dependencies>
  </metadata>
</package>